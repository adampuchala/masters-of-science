{
    "compilerOptions": {
      "moduleResolution": "node",
      "module": "es2020",
      "outDir": "static/script",
      "rootDir": "src/typescript/",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "noEmit": false,
      "noEmitOnError": false
    },
    "include": [
      "src/typescript/"
    ],
    "exclude": [
      "src/typescript/specs/"
    ]
}
